<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context=".Activities.Extra.TestActivity">


    <TextView
        android:id="@+id/id_title"
        android:textSize="10dp"
        android:text="id_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/id_body"
        android:textSize="10dp"
        android:text="id_body"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/id_Paciente"
        android:textSize="10dp"
        android:text="id_Paciente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/id_ubicacion"
        android:textSize="10dp"
        android:text="id_ubicacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />



</LinearLayout>