<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Business.DoctorRuta">


    <fragment
        android:id="@+id/mapDoctorTracking"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".DoctorTracking" />

    <Button
        android:id="@+id/btnSendNotiArrived"
        android:layout_width="150dp"
        android:layout_height="64dp"
        android:layout_alignBottom="@id/mapDoctorTracking"
        android:layout_centerHorizontal="true"
        android:layout_margin="6dp"
        android:background="@drawable/custom_button_5"
        android:enabled="false"
        android:text="Notificar"
        android:textColor="@color/white"
        android:visibility="invisible" />


</RelativeLayout>